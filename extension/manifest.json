{
    "manifest_version": 2,
    "name": "Genie Music Scrapper",
    "version": "0.1",
    "description": "Generate a plain text list of your Genie Music playlist.",
    "background": {
        "persistent": true,
        "scripts": ["js/background.js"]
      },
    "permissions": [
         "contextMenus",
         "activeTab"
       ],
    "icons": {
         "16": "icons/icon16x16.png",
         "48": "icons/icon48x48.png",
         "128": "icons/icon128x128.png"
         }
}